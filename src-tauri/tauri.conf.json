{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "build": {
    "beforeBuildCommand": "bun run build",
    "beforeDevCommand": "bun run dev",
    "frontendDist": "../.output/public",
    "devUrl": "http://localhost:3000/"
  },
  "bundle": {
    "active": true,
    "category": "DeveloperTool",
    "copyright": "",
    "targets": "all",
    "externalBin": [],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": ""
    },
    "longDescription": "",
    "macOS": {
      "entitlements": null,
      "exceptionDomain": "",
      "frameworks": [],
      "providerShortName": null,
      "signingIdentity": null
    },
    "resources": [],
    "shortDescription": "",
    "linux": {
      "deb": {
        "depends": []
      }
    },
    "createUpdaterArtifacts": "v1Compatible"
  },
  "productName": "mahalli",
  "mainBinaryName": "mahalli",
  "version": "1.9.6",
  "identifier": ".mahalli",
  "plugins": {
    "updater": {
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDdGMTJGNUYzMjQ4MEIxRDEKUldUUnNZQWs4L1VTZjNMb3VKWCs1OHJXUWlzVUR3U0JUT29GcGd1WU9hZ2hLNGdwSEJ2eWNZOVUK",
      "endpoints": [
        "https://mahalli-dektop-updater.abdelilah4dev.workers.dev/check/AbdelilahOu/Mahalli-nuxt/{{target}}/{{arch}}/{{current_version}}"
      ],
      "windows": {
        "installMode": "basicUi"
      }
    }
  },
  "app": {
    "windows": [
      {
        "fullscreen": false,
        "height": 750,
        "resizable": true,
        "title": "Mahalli | inventory & invoicing",
        "width": 1000
      }
    ],
    "security": {
      "assetProtocol": {
        "scope": ["**"],
        "enable": true
      },
      "csp": "default-src 'self' blob: data: filesystem: ws: wss: http: https: tauri: 'unsafe-eval' 'unsafe-inline'; script-src-elem 'self' https://accounts.google.com 'unsafe-eval' 'unsafe-hashes' 'unsafe-inline'; style-src 'self' https://accounts.google.com 'unsafe-eval' 'unsafe-hashes' 'unsafe-inline';; connect-src ipc: http://ipc.localhost"
    }
  }
}
